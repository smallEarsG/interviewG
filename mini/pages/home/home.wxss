/* pages/home/home.wxss */
.container{
  width: 100%;
  background: rgb(236, 233, 233);
  position: absolute;
  bottom: 0;
  top: 0;
}
.header{
  display: flex;
  /* justify-content: space-between; */
  background-color: rgb(16, 20, 44);
  color: #fff;
  padding: 0 20rpx;
  align-items: center;
  box-sizing: border-box;
  height: 100rpx;
}
.title{
  color:#3aa71e;
  flex: 1;
  text-align: center;
}
.tips{
  position: relative;
}
.tips .tipsIcon{
  width: 50rpx;
  height: 50rpx;
}
.number{
  position: absolute;
  top: -10rpx;
  background: red;
  font-size: 18rpx;
  border-radius: 50%;
  text-align: center;
  /* overflow: hidden; */
  left: 25rpx;
  line-height: 34rpx;
  width: 34rpx;
  height: 34rpx;
}
image{
  width: 100%;
}
.nav{
  display: flex;
  justify-content: space-around;
  background-color: rgb(16, 20, 44);
  color: #999;
  padding: 10rpx 0;
}
.navItem{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.navIcom image{
  width: 100rpx;
  height: 100rpx;
  
}
.content{
  background-color: #fff;
}
.content-hander{
  display: flex;
}
.dialogItem{
  flex: 1;
  border-right:1px solid #999 ;
  border-bottom:1px solid #999 ;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.imgcontainer{
  display: flex;
  align-items: center;
  /* width: 100rpx; */
  /* height: 100rpx; */
}
.imgbox{
 display: flex;
 flex-direction: column;
}
.img{
  width: 50rpx;
  height: 50rpx;
  background-color: green;
  margin: 5rpx;
  overflow: hidden;
  box-sizing: border-box;
}
image{
  width: 100%;
  height: 100%;
}
.leftTopImg{
  border-top-left-radius: 43%;
  border-bottom-left-radius: 43%;
  border-top-right-radius: 43%;
}
.rightTopImg{
  border-top-left-radius: 43%;
  border-bottom-right-radius: 43%;
  border-top-right-radius: 43%;
}
.leftBottomImg{
  border-top-left-radius: 43%;
  border-bottom-left-radius: 43%;
  border-bottom-right-radius: 43%;
}
.rightBottomImg{
  border-top-right-radius: 43%;
  border-bottom-right-radius: 43%;
  border-bottom-left-radius: 43%;
}
.swiper-item{
  width: 100%;
  height: 100%;
}
.cootent-other{
  width: 100%;
}
.othercontainer{
  display: flex;
}
.otherItem{
  flex: 1;
  align-items: center;
  display: flex;
  flex-direction: column;
  border-right:1px solid #999 ;
  border-bottom:1px solid #999 ;
}
.otherIcon{
  width: 116rpx;
  height: 116rpx;
}